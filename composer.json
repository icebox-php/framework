{
    "name": "icebox-php/framework",
    "type": "library",
    "description": "The Icebox Framework.",
    "keywords": ["framework", "icebox"],
    "license": "MIT",
    "homepage": "http://torovin.com/icebox",
    "require": {
        "vlucas/phpdotenv": "^5.6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "12.5.3",
        "psy/psysh": "^0.12.17"
    },
    "autoload": {
        "files": [
            "src/helper.php"
        ],
        "psr-4": {
          "Icebox\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Icebox\\Tests\\": "tests/"
        }
    }
}
